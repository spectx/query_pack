@[./view/view_example_data.sx](from:now()[-30 day], to:now(), app:'user')
| select(last(accounts[last_login_time]),*)
| group(email)
| sort(last_login_time)
| filter(last_login_time = var("1970-01-01 00:00:00.000 +0000"))
