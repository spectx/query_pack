// @[./view/view_example_data.sx] (from:now()[-2 day], to:now(), app:'customer')
// @[./view/view_example_data.sx] (from:now()[-2 day], to:now())
@[./view/view_example_data.sx](from:now()[-3 day], to:now(), app:'user')
| select(last_login_time:STR_TO_TIME(STRING(MAX(accounts[last_login_time]))),email)
| group(email)
| sort(last_login_time)
| filter(last_login_time = T("1970-01-01 00:00:00.000 +0000"))

