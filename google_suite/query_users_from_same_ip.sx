// @[./view/view_example_data.sx] (from:now()[-2 day], to:now(),app:'drive')
// @[./view/view_example_data.sx] (from:now()[-2 day], to:now())
@[./view/view_example_data.sx](app:'login')
| select(ipAddress:IP_TRUNC(ipAddress,32,64), cc(ipAddress), asname(ipAddress), geo(ipAddress), *)
| group(email, ipAddress)
| select(ipAddress, users:ARRAY_AGG(email), cc, asname, geo)
| group(ipAddress)
| select(SIZE(users),*)
| filter(SIZE(users) > 1)
| sort(SIZE(users) desc)
